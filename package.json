{
  "name": "db2-intership",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 src/styles.scss -o assets/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace assets/css/styles.css --use autoprefixer --map",
    "css-purge": "purgecss --css assets/css/styles.css --content index.html --output assets/css/",
    "css": "npm-run-all css-compile css-prefix",
    "server": "serve --listen 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "browser-sync": "^2.26.14",
    "css-loader": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-watch": "^5.0.1",
    "imagemin-pngquant": "^9.0.1",
    "modern-css-reset": "^1.4.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss": "^4.0.2",
    "sass-loader": "^11.0.1",
    "sassdoc": "^2.7.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0"
  }
}
